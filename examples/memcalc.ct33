// adding and shifting values read from memory
// where results are stored in memory
    JMP start

shiftresult:
.shifted:  DAT 0
.overflow: DAT 0

first:
    DAT 1234
second:
    DAT 2325
added:
    DAT 0

start:
    LOAD x1, first
    LOAD x2, second
    ADD  x3, x1, x2
    STOR x3, added
    MOVE x6, shiftresult
    SHFT x5, x2, 2
    STOR x2, x6, .shifted
    STOR x5, x6, .overflow
    HLT

