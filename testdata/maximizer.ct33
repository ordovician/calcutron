    MOVE x9, tape
    MOVE x8, first
    MOVE x7, second
loop:
    LOAD x1, x9      // read punched tap into x1
    LOAD x2, x9      // load tape input into x2 
    BGT  x8, x1, x2  // goto first if x1 > x2

second:
    STORE x2, x9     // write result out to tape
    JUMP x0, loop
    
first:
    STORE x1, x9
    JUMP x0, loop
